            android:name="com.example.add.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.add.DisplayResult"
            android:label="@string/title_activity_display_result" >
        </activity>
        <activity
            android:name="com.example.add.Shopping"
            android:label="@string/title_activity_shopping" >
        </activity>
        <activity
            android:name="com.example.add.HomeUti"
            android:label="@string/title_activity_home_uti" >
        </activity>
        <activity
            android:name="com.example.add.Education"
            android:label="@string/title_activity_education" >
        </activity>
        <activity
            android:name="com.example.add.Entertainment"
            android:label="@string/title_activity_entertainment" >
        </activity>
         <activity
            android:name="org.achartengine.GraphicalActivity">
        </activity>
        <receiver android:name="com.example.Add.AlarmNotification" >
        </receiver>
    </application>

</manifest>
